<configuration>

  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <!-- encoders are assigned the type
         ch.qos.logback.classic.encoder.PatternLayoutEncoder by default -->
    <encoder>
      <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} [%M] - %msg%n</pattern>
    </encoder>
  </appender>
  
<!--  <logger name="de.julielab.costosys.dbconnection.CoStoSysConnection" level="TRACE"/>-->
<!--  <logger name="de.julielab.costosys.dbconnection.DataBaseConnector" level="TRACE"/>-->
  <root level="INFO">
    <appender-ref ref="STDOUT" />
  </root>
</configuration>